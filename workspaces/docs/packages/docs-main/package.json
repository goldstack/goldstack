{
  "name": "@goldstack/docs-main",
  "version": "0.1.0",
  "license": "",
  "author": "",
  "sideEffects": false,
  "scripts": {
    "build": "yarn clean && yarn compile",
    "build:next": "next build && rimraf ./webDist && next export -o webDist/",
    "clean": "rimraf ./dist ./webDist",
    "compile": "tsc -p tsconfig.json",
    "compile-watch": "tsc -p tsconfig.compile.json --watch",
    "compile:sass": "node-sass src/styles/app.scss src/styles/app.css --source-map src/styles/app.css.map",
    "coverage": "jest --collect-coverage --passWithNoTests --config=./jest.config.js --runInBand",
    "deploy": "yarn template set-nextjs-env $@ && yarn build && yarn web:build && yarn template-ts deploy $@",
    "deploy-ts": "yarn template-ts set-nextjs-env $@ && yarn build && yarn web:build && yarn template-ts deploy $@",
    "generate-docs": "ts-node --project tsconfig.local.json ./scripts/generateDocs.ts",
    "infra": "yarn template infra",
    "next:tracking:disable": "next telemetry disable",
    "next:watch": "next",
    "prepublishOnly": "yarn run build",
    "start:next": "next start",
    "template": "yarn template-ts",
    "template-ts": "ts-node --project tsconfig.local.json scripts/template.ts",
    "test": "jest --config=./jest.config.js --watch",
    "test-ci": "jest --passWithNoTests --config=./jest.config.js --runInBand",
    "watch": "yarn next:watch -- -p 3001",
    "web:build": "yarn build:next",
    "web:watch": "yarn web:build && concurrently \"reload --dir ./webDist --watch-dir ./webDist --port 8071 --browser\" "
  },
  "dependencies": {
    "@fullstory/browser": "^1.4.5",
    "@goldstack/toc-generator": "0.1.0",
    "@goldstack/utils-track": "0.2.9",
    "cheerio": "1.0.0-rc.10",
    "next": "13.5.1",
    "react": "18.2.0",
    "react-bootstrap": "^1.6.1",
    "react-dom": "18.2.0",
    "react-is": "^18.2.0",
    "styled-components": "^5.3.5",
    "swr": "^0.4.0"
  },
  "devDependencies": {
    "@goldstack/markdown-docs": "0.1.0",
    "@goldstack/template-nextjs": "0.5.32",
    "@goldstack/utils-sh": "0.5.11",
    "@swc/core": "^1.3.74",
    "@swc/jest": "^0.2.27",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.3.0",
    "@types/cheerio": "^0.22.31",
    "@types/jest": "^29.0.1",
    "@types/node": "^18.7.13",
    "@types/react": "^18.0.15",
    "@types/styled-components": "^5.1.25",
    "@types/testing-library__jest-dom": "^5.14.5",
    "@types/testing-library__react": "^10.2.0",
    "babel-plugin-styled-components": "^2.0.7",
    "concurrently": "^7.3.0",
    "file-loader": "^6.2.0",
    "jest": "^29.3.1",
    "jest-environment-jsdom": "^28.1.3",
    "jest-transform-stub": "^2.0.0",
    "next-compose-plugins": "^2.2.1",
    "node-sass": "^8.0.0",
    "reload": "^3.2.0",
    "rimraf": "^3.0.2",
    "svg-url-loader": "^8.0.0",
    "ts-node": "^10.9.1",
    "typescript": "^4.8.4"
  }
}
