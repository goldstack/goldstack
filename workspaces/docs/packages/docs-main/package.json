{
  "name": "@goldstack/docs-main",
  "version": "0.1.0",
  "license": "",
  "author": "",
  "sideEffects": false,
  "scripts": {
    "build": "yarn clean && yarn compile",
    "build:next": "NEXT_TELEMETRY_DISABLED=1 rimraf ./webDist && next build",
    "clean": "rimraf ./dist ./webDist",
    "clean-terraform": "rimraf ./infra/aws/.terraform",
    "compile": "tsc -p tsconfig.json",
    "compile-watch": "tsc -p tsconfig.compile.json --watch",
    "compile:sass": "sass src/styles/app.scss src/styles/app.css --source-map ",
    "coverage": "jest --collect-coverage --passWithNoTests --config=./jest.config.js --runInBand",
    "deploy": "yarn template set-nextjs-env $@ && yarn build && yarn web:build && yarn template-ts deploy $@",
    "deploy-ts": "yarn template-ts set-nextjs-env $@ && yarn build && yarn web:build && yarn template-ts deploy $@",
    "generate-docs": "ts-node --project tsconfig.local.json ./scripts/generateDocs.ts",
    "infra": "yarn template infra",
    "next:tracking:disable": "next telemetry disable",
    "next:watch": "next -p 3001",
    "prepublishOnly": "yarn run build",
    "start:next": "next start",
    "template": "yarn template-ts",
    "template-ts": "ts-node --project tsconfig.local.json scripts/template.ts",
    "test": "jest --passWithNoTests --config=./jest.config.js --runInBand",
    "test-watch": "jest --config=./jest.config.js --watch",
    "watch": "yarn next:watch",
    "web:build": "yarn build:next",
    "web:watch": "yarn web:build && concurrently \"reload --dir ./webDist --watch-dir ./webDist --port 8071 --browser\" "
  },
  "dependencies": {
    "@fullstory/browser": "^1.4.5",
    "@goldstack/toc-generator": "0.1.0",
    "@goldstack/utils-track": "0.2.15",
    "cheerio": "1.0.0-rc.10",
    "next": "^15.4.1",
    "react": "^19.1.0",
    "react-bootstrap": "^2.10.10",
    "react-dom": "^19.1.0",
    "react-is": "^19.1.0",
    "styled-components": "^5.3.5",
    "swr": "^0.4.0"
  },
  "devDependencies": {
    "@goldstack/markdown-docs": "0.1.0",
    "@goldstack/template-nextjs": "0.5.63",
    "@goldstack/utils-sh": "0.5.23",
    "@swc/core": "^1.12.11",
    "@swc/jest": "^0.2.39",
    "@testing-library/dom": "^10.4.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.1.0",
    "@types/cheerio": "^0.22.35",
    "@types/jest": "^29.5.14",
    "@types/node": "^24.0.13",
    "@types/react": "18",
    "@types/styled-components": "5.1.25",
    "@types/testing-library__jest-dom": "5",
    "@types/testing-library__react": "^10.2.0",
    "babel-plugin-styled-components": "^2.0.7",
    "concurrently": "^7.3.0",
    "file-loader": "^6.2.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "jest-transform-stub": "^2.0.0",
    "next-compose-plugins": "^2.2.1",
    "reload": "^3.2.0",
    "rimraf": "^3.0.2",
    "sass": "^1.89.2",
    "svg-url-loader": "^8.0.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.3"
  }
}
