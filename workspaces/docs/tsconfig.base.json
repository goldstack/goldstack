{
  "compilerOptions": {
    "incremental": true,
    "target": "ES2019",
    "composite": true,
    "noEmit": false,
    "module": "commonjs",
    "lib": ["dom", "ESNext"],
    "allowJs": true,
    "checkJs": false,
    "declaration": true,
    "declarationMap": true,
    "sourceMap": true,
    "skipLibCheck": true,
    "strict": true,
    "noImplicitAny": true,
    "strictNullChecks": true,
    "useUnknownInCatchVariables": false,
    "esModuleInterop": true,
    "resolveJsonModule": true,
    "forceConsistentCasingInFileNames": true,
    "types": ["jest", "node"]
  }
}
