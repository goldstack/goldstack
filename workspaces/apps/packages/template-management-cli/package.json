{
  "name": "@goldstack/template-management-cli",
  "version": "0.1.0",
  "license": "",
  "author": "",
  "sideEffects": false,
  "main": "src/image.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "yarn clean && yarn compile",
    "clean": "rimraf ./dist",
    "cli": "ts-node --swc ./src/cli.ts",
    "cli-deploy-set-dev": "yarn cli deploy-set --set nextjs-bootstrap --repo goldstack-dev --deployment dev --workDir tmp --skipTests false",
    "cli-schedule-all-deploy-sets-dev": "yarn cli schedule-all-deploy-sets --repo goldstack-dev --deployment dev",
    "compile": "tsc --build",
    "compile-watch": "tsc --build --watch",
    "coverage": "jest --collect-coverage --passWithNoTests --config=./jest.config.js --runInBand",
    "infra": "yarn template infra",
    "test": "jest --passWithNoTests --config=./jest.config.js --runInBand"
  },
  "dependencies": {
    "@aws-sdk/client-ses": "3.934.0",
