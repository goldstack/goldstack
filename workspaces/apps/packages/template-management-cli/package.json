{
  "name": "@goldstack/template-management-cli",
  "version": "0.1.0",
  "license": "",
  "author": "",
  "sideEffects": false,
  "main": "src/image.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "yarn clean && yarn compile",
    "build-docker": "ts-node ./src/build.ts build $@",
    "build:pack": "yarn compile && yarn webpack",
    "clean": "rm -rf ./dist && yarn clean-docker",
    "clean-docker": "ts-node ./src/build.ts clean $@",
    "cli": "ts-node ./src/cli.ts",
    "cli-deploy-set-backend-dev": "yarn cli deploy-set --set backend --repo goldstack-dev --deployment dev --emailResultsTo maxrohde.public@gmail.com --workDir tmp --skipTests false",
    "cli-schedule-all-deploy-sets-dev": "yarn cli schedule-all-deploy-sets --repo goldstack-dev --deployment dev",
    "compile": "tsc --build",
    "compile-watch": "tsc --build --watch",
    "coverage": "jest --collect-coverage --passWithNoTests --config=./jest.config.js",
    "deploy": "yarn build && yarn build-docker $@ && yarn template deploy $@",
    "deploy-ts": "yarn build && yarn build-docker $@ && yarn template-ts deploy $@",
    "infra": "yarn template infra",
    "template": "template-docker-image-aws",
    "template-ts": "ts-node scripts/template.ts",
    "test-ci": "jest --passWithNoTests --config=./jest.config.js"
  },
  "dependencies": {
    "@goldstack/goldstack-email-send": "0.1.0",
    "@goldstack/infra-aws": "0.3.25",
    "@goldstack/project-build": "0.1.0",
    "@goldstack/project-install": "0.1.0",
    "@goldstack/template-build-set": "0.1.0",
    "@goldstack/template-repository": "0.1.0",
    "@goldstack/template-repository-bucket": "0.1.0",
    "@goldstack/template-s3": "0.3.59",
    "@goldstack/template-static-website-aws": "0.3.59",
    "@goldstack/utils-cli": "0.2.7",
    "@goldstack/utils-config": "0.3.20",
    "@goldstack/utils-git": "0.1.28",
    "@goldstack/utils-package": "0.3.21",
    "@goldstack/utils-project": "0.3.21",
    "mock-aws-s3": "^4.0.2",
    "source-map-support": "^0.5.20",
    "tmp": "^0.2.1",
    "yargs": "^15.4.1"
  },
  "devDependencies": {
    "@goldstack/template-docker-image-aws": "0.3.42",
    "@goldstack/utils-sh": "0.4.19",
    "@types/jest": "^27.0.2",
    "@types/mock-aws-s3": "^2.6.1",
    "@types/node": "^16.11.0",
    "@types/source-map-support": "^0.5.4",
    "@types/wtfnode": "^0.7.0",
    "@types/yargs": "^15.0.5",
    "aws-sdk": "^2.995.0",
    "concurrently": "^5.2.0",
    "jest": "^27.4.3",
    "nodemon": "^2.0.4",
    "pnp-webpack-plugin": "^1.6.4",
    "ts-jest": "^26.5.4",
    "ts-node": "^10.3.0",
    "typescript": "^4.4.3",
    "webpack": "4.43.0",
    "webpack-cli": "3.3.12"
  }
}
