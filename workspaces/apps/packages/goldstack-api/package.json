{
  "name": "@goldstack/goldstack-api",
  "version": "0.1.0",
  "description": "Goldstack REST API",
  "license": "MIT",
  "author": "Max Rohde",
  "sideEffects": false,
  "main": "src/module.ts",
  "scripts": {
    "build": "ts-node ./scripts/build.ts",
    "clean": "rm -rf ./dist && rm -rf distLambda",
    "compile": "tsc --build",
    "coverage": "jest --collect-coverage --passWithNoTests --config=./jest.config.js",
    "deploy": "yarn build && yarn template deploy",
    "deploy-ts": "yarn build && yarn template-ts deploy",
    "infra": "template infra",
    "seed-modules": "GOLDSTACK_DEPLOYMENT=local ts-node ./src/utils/seedRepo.ts",
    "start": "yarn node dist/src/local.js",
    "template-ts": "ts-node scripts/template.ts",
    "test": "GOLDSTACK_DEPLOYMENT=local CORS=http://localhost jest --passWithNoTests --watch --config=jest.config.js",
    "test-ci": "GOLDSTACK_DEPLOYMENT=local CORS=http://localhost jest --passWithNoTests --config=jest.config.js --detectOpenHandles",
    "watch": "PORT=8731 CORS=http://localhost:3000 GOLDSTACK_DEPLOYMENT=local GOLDSTACK_LOG_EMAILS=true ts-node-dev ./src/local.ts"
  },
  "dependencies": {
    "@goldstack/auth": "0.1.0",
    "@goldstack/infra-aws": "0.3.29",
    "@goldstack/project-build": "0.1.0",
    "@goldstack/project-config": "0.1.0",
    "@goldstack/project-package-bucket": "0.1.0",
    "@goldstack/project-repository": "0.1.0",
    "@goldstack/session-repository": "0.1.0",
    "@goldstack/template-build": "0.1.0",
    "@goldstack/template-lambda-express": "0.3.76",
    "@goldstack/template-repository": "0.1.0",
    "@goldstack/template-repository-bucket": "0.1.0",
    "@goldstack/utils-package": "0.3.24",
    "@goldstack/utils-project": "0.3.24",
    "@goldstack/utils-sh": "0.4.22",
    "@goldstack/utils-template": "0.3.24",
    "aws-serverless-express": "^3.3.8",
    "cookie-parser": "^1.4.5",
    "cors": "^2.8.5",
    "crypto-random-string": "^3.2.0",
    "express-requests-logger": "^3.0.2",
    "helmet": "^4.6.0",
    "sanitize-html": "^2.4.0",
    "source-map-support": "^0.5.21",
    "stripe": "^8.114.0",
    "uuid": "^8.3.0"
  },
  "devDependencies": {
    "@goldstack/goldstack-email-send": "0.1.0",
    "@goldstack/module-template-utils": "0.1.0",
    "@goldstack/utils-template-test": "0.1.0",
    "@jest-mock/express": "^1.1.7",
    "@types/aws-serverless-express": "^3.3.3",
    "@types/cookie-parser": "^1.4.2",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/helmet": "^4.0.0",
    "@types/jest": "^27.0.2",
    "@types/node": "^16.11.0",
    "@types/sanitize-html": "^1.23.3",
    "@types/source-map-support": "^0.5.4",
    "@types/supertest": "^2.0.10",
    "@types/tsc-watch": "^4",
    "@types/uuid": "^8.3.0",
    "@yarnpkg/esbuild-plugin-pnp": "^2.0.0-rc.1",
    "esbuild": "^0.14.5",
    "express": "^4.17.1",
    "jest": "^26.6.3",
    "supertest": "^4.0.2",
    "supertest-session": "^4.0.0",
    "ts-jest": "^26.5.4",
    "ts-node": "^10.3.0",
    "ts-node-dev": "^1.1.8",
    "typescript": "^4.4.3"
  }
}
