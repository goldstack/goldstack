{
  "name": "@goldstack/goldstack-api",
  "version": "0.1.0",
  "description": "Goldstack REST API",
  "license": "MIT",
  "author": "Max Rohde",
  "sideEffects": false,
  "main": "src/module.ts",
  "scripts": {
    "build": "ts-node ./scripts/build.ts",
    "clean": "rimraf ./dist && rimraf distLambda && rimraf ./goldstackLocal",
    "clean-terraform": "rimraf ./infra/aws/.terraform",
    "compile": "tsc --build",
    "coverage": "jest --collect-coverage --passWithNoTests --config=./jest.config.js --runInBand",
    "deploy": "yarn build && yarn template deploy",
    "deploy-ts": "yarn build && yarn template-ts deploy",
    "infra": "yarn template infra",
    "seed-templates": "GOLDSTACK_DEPLOYMENT=local ts-node --swc ./src/utils/seedRepo.ts",
    "start": "yarn node dist/src/local.js",
    "template": "yarn template-ts",
    "template-ts": "ts-node --swc scripts/template.ts",
    "test": "GOLDSTACK_DEPLOYMENT=local CORS=http://localhost jest --passWithNoTests --config=jest.config.js --detectOpenHandles --runInBand",
    "test-watch": "GOLDSTACK_DEPLOYMENT=local CORS=http://localhost jest --passWithNoTests --watch --config=jest.config.js",
    "watch": "PORT=8731 CORS=http://localhost:3000 GOLDSTACK_DEPLOYMENT=local GOLDSTACK_LOG_EMAILS=true ts-node-dev --respawn ./src/local.ts"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.844.0",
    "@aws-sdk/client-ses": "^3.844.0",
    "@goldstack/auth": "0.1.0",
    "@goldstack/infra-aws": "0.4.34",
    "@goldstack/project-build": "0.1.0",
    "@goldstack/project-config": "0.1.0",
    "@goldstack/project-package-bucket": "0.1.0",
    "@goldstack/project-repository": "0.1.0",
    "@goldstack/session-repository": "0.1.0",
    "@goldstack/template-build": "0.1.0",
    "@goldstack/template-lambda-http-cli": "0.5.58",
    "@goldstack/template-repository": "0.1.0",
    "@goldstack/template-repository-bucket": "0.1.0",
    "@goldstack/utils-package": "0.4.22",
    "@goldstack/utils-project": "0.4.22",
    "@goldstack/utils-sh": "0.5.21",
    "@goldstack/utils-template": "0.4.22",
    "aws-serverless-express": "^3.3.8",
    "cookie-parser": "^1.4.5",
    "cors": "^2.8.5",
    "crypto-random-string": "^3.2.0",
    "helmet": "^4.6.0",
    "sanitize-html": "^2.4.0",
    "sort-package-json": "^3.4.0",
    "source-map-support": "^0.5.21",
    "stripe": "^8.114.0",
    "uuid": "^11.0.5"
  },
  "devDependencies": {
    "@goldstack/goldstack-email-send": "0.1.0",
    "@goldstack/module-template-utils": "0.1.0",
    "@goldstack/utils-template-test": "0.1.0",
    "@jest-mock/express": "^1.4.5",
    "@swc/core": "^1.12.11",
    "@swc/jest": "^0.2.39",
    "@types/aws-serverless-express": "^3.3.10",
    "@types/cookie-parser": "^1.4.8",
    "@types/cors": "^2.8.17",
    "@types/express": "^5.0.0",
    "@types/helmet": "^4.0.0",
    "@types/jest": "^29.5.14",
    "@types/node": "^24.0.13",
    "@types/rimraf": "^4.0.5",
    "@types/sanitize-html": "^2.13.0",
    "@types/source-map-support": "^0.5.10",
    "@types/supertest": "^6.0.2",
    "@types/tsc-watch": "^4.2.3",
    "@types/uuid": "^10.0.0",
    "@yarnpkg/esbuild-plugin-pnp": "^3.0.0-rc.15",
    "esbuild": "^0.24.2",
    "express": "4.19.2",
    "jest": "^29.7.0",
    "rimraf": "^3.0.2",
    "supertest": "^4.0.2",
    "supertest-session": "^4.0.0",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.8.3"
  }
}
