{
  "name": "@goldstack/template-docker-image-aws",
  "version": "0.5.60",
  "description": "Utilities for packages that are deployed as Docker Images using AWS Fargate",
  "keywords": [
    "goldstack",
    "utility",
    "infrastructure",
    "IaC",
    "fargate",
    "ecs",
    "aws",
    "docker",
    "configuration"
  ],
  "homepage": "https://goldstack.party",
  "bugs": {
    "url": "https://github.com/goldstack/goldstack/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/goldstack/goldstack.git"
  },
  "license": "MIT",
  "author": "Max Rohde",
  "sideEffects": false,
  "main": "src/templateDockerImageAws.ts",
  "bin": {
    "template": "./bin/template",
    "template-docker-image-aws": "./bin/template"
  },
  "scripts": {
    "build": "yarn clean && yarn compile",
    "build:watch": "yarn clean && yarn compile-watch",
    "clean": "rimraf ./dist",
    "compile": "tsc -p tsconfig.json",
    "compile-watch": "tsc -p tsconfig.json --watch",
    "compile-watch:light": "nodemon --watch ./src/ -e '*' --exec 'yarn compile'",
    "coverage": "jest --collect-coverage --passWithNoTests --config=./jest.config.js --runInBand",
    "generate-schema": "ts-node scripts/generateSchemas.ts AWSDockerImage && cp schemas/* ../../../templates/packages/docker-image-aws/schemas",
    "prepublishOnly": "yarn run build",
    "publish": "utils-git changed --exec \"yarn npm publish $@\"",
    "test": "jest --passWithNoTests --config=./jest.config.js --runInBand",
    "version:apply": "utils-git changed --exec \"yarn version $@ && yarn version apply\"",
    "version:apply:force": "yarn version $@ && yarn version apply"
  },
  "dependencies": {
    "@goldstack/infra": "0.4.28",
    "@goldstack/infra-aws": "0.4.42",
    "@goldstack/utils-aws-cli": "0.4.43",
    "@goldstack/utils-cli": "0.3.22",
    "@goldstack/utils-config": "0.4.28",
    "@goldstack/utils-docker": "0.4.29",
    "@goldstack/utils-git": "0.2.18",
    "@goldstack/utils-log": "0.3.24",
    "@goldstack/utils-package": "0.4.28",
    "@goldstack/utils-package-config": "0.4.28",
    "@goldstack/utils-s3-deployment": "0.5.46",
    "@goldstack/utils-sh": "0.5.27",
    "@goldstack/utils-template": "0.4.28",
    "@goldstack/utils-terraform": "0.4.54",
    "@goldstack/utils-terraform-aws": "0.4.57",
    "aws-sdk": "2.1525.0",
    "source-map-support": "^0.5.21",
    "yargs": "^17.5.1"
  },
  "devDependencies": {
    "@goldstack/utils-git": "0.2.18",
    "@goldstack/utils-package-config-generate": "0.3.19",
    "@swc/core": "^1.12.11",
    "@swc/jest": "^0.2.39",
    "@types/jest": "^29.5.14",
    "@types/node": "^24.0.13",
    "@types/source-map-support": "^0.5.10",
    "@types/yargs": "^17.0.33",
    "jest": "^29.7.0",
    "rimraf": "^3.0.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.3"
  },
  "publishConfig": {
    "main": "dist/src/templateDockerImageAws.js"
  }
}
