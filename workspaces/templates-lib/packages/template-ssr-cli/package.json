{
  "name": "@goldstack/template-ssr-cli",
  "version": "0.3.105",
  "description": "CLI tools for server-side rendering template",
  "keywords": [
    "goldstack",
    "utility",
    "infrastructure",
    "ssr",
    "aws",
    "IaC",
    "configuration"
  ],
  "homepage": "https://goldstack.party",
  "bugs": {
    "url": "https://github.com/goldstack/goldstack/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/goldstack/goldstack.git"
  },
  "license": "MIT",
  "author": "Max Rohde",
  "sideEffects": false,
  "main": "src/templateSSRCli.ts",
  "scripts": {
    "build": "yarn clean && yarn compile",
    "build:watch": "yarn clean && yarn compile-watch",
    "clean": "rimraf ./dist",
    "compile": "tsc -p tsconfig.json",
    "compile-watch": "tsc -p tsconfig.json --watch",
    "compile-watch:light": "nodemon --watch ./src/ -e '*' --exec 'yarn compile'",
    "coverage": "jest --collect-coverage --passWithNoTests --config=./jest.config.js --runInBand",
    "prepublishOnly": "yarn run build",
    "publish": "yarn npm publish --tolerate-republish",
    "test": "jest --passWithNoTests --config=./jest.config.js --runInBand",
    "patch-when-changed": "git diff --quiet HEAD^...HEAD -- . && git diff --quiet -- . && git diff --quiet --staged -- .  || yarn version patch"
  },
  "dependencies": {
    "@goldstack/infra": "0.4.39",
    "@goldstack/infra-aws": "0.4.58",
    "@goldstack/template-ssr": "0.3.98",
    "@goldstack/template-ssr-server": "0.3.96",
    "@goldstack/template-ssr-server-compile-bundle": "0.3.93",
    "@goldstack/utils-aws-lambda": "0.3.71",
    "@goldstack/utils-cli": "0.3.31",
    "@goldstack/utils-docker": "0.4.40",
    "@goldstack/utils-log": "0.3.33",
    "@goldstack/utils-package": "0.4.41",
    "@goldstack/utils-package-config": "0.4.41",
    "@goldstack/utils-s3-deployment": "0.5.62",
    "@goldstack/utils-sh": "0.5.38",
    "@goldstack/utils-template": "0.4.40",
    "@goldstack/utils-terraform": "0.4.72",
    "@goldstack/utils-terraform-aws": "0.4.76",
    "outmatch": "^1.0.0",
    "source-map-support": "^0.5.21",
    "yargs": "^17"
  },
  "devDependencies": {
    "@goldstack/utils-package-config-generate": "0.3.28",
    "@swc/core": "^1.15.8",
    "@swc/jest": "^0.2.39",
    "@types/jest": "^30.0.0",
    "@types/node": "^25.0.3",
    "@types/yargs": "^17.0.33",
    "jest": "^30.2.0",
    "rimraf": "^6.1.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.3"
  },
  "publishConfig": {
    "main": "dist/src/templateSSRCli.js"
  }
}
