  "devDependencies": {
    "@goldstack/template-s3-cli": "0.6.15",
    "@swc/core": "^1.3.74",
    "@swc/jest": "^0.2.27",
    "@types/jest": "^29.0.1",
    "@types/node": "^18.7.13",
    "aws-sdk": "2.1525.0",
    "concurrently": "^7.3.0",
    "jest": "^29.3.1",
    "rimraf": "^3.0.2",
    "ts-node": "^10.9.1",
    "typescript": "^4.8.4"
  }
