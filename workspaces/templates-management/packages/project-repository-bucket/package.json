    "@smithy/types": "4.9.0"
  },
  "devDependencies": {
    "@goldstack/template-s3-cli": "0.6.68",
    "@swc/core": "^1.12.11",
    "@swc/jest": "^0.2.39",
    "@types/jest": "^29.5.14",
    "@types/node": "^24.0.13",
    "concurrently": "^7.3.0",
    "jest": "^29.7.0",
    "rimraf": "^3.0.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.3"
