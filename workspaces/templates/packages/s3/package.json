{
  "name": "@goldstack/s3",
  "version": "0.1.0",
  "private": true,
  "license": "MIT",
  "author": "Max Rohde",
  "sideEffects": false,
  "main": "src/bucket.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "yarn clean && yarn compile",
    "clean": "rimraf dist/",
    "compile": "tsc -p tsconfig.json",
    "compile-watch": "tsc -p tsconfig.json --watch",
    "coverage": "jest --collect-coverage --passWithNoTests --config=./jest.config.js --runInBand",
    "deploy": "template deploy $@",
    "infra": "template infra",
    "template": "template",
    "template-ts": "ts-node scripts/template.ts",
    "test-ci": "GOLDSTACK_DEPLOYMENT=local jest --passWithNoTests --config=./jest.config.js --runInBand"
  },
  "dependencies": {
    "@goldstack/template-s3": "0.4.4",
    "aws-sdk": "^2.1222.0"
  },
  "devDependencies": {
    "@goldstack/template-s3-cli": "0.6.4",
    "@types/jest": "^29.0.1",
    "@types/node": "^18.7.13",
    "jest": "^28.1.0",
    "rimraf": "^3.0.2",
    "ts-jest": "^28.0.2",
    "ts-node": "^10.9.1",
    "typescript": "^4.7.4"
  }
}
