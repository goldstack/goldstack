{
  "name": "@goldstack/s3",
  "version": "0.1.0",
  "private": true,
  "license": "MIT",
  "author": "Max Rohde",
  "sideEffects": false,
  "main": "src/bucket.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "yarn clean && yarn compile",
    "clean": "rimraf dist/",
    "clean-terraform": "rimraf ./infra/aws/.terraform",
    "compile": "tsc -p tsconfig.json",
    "compile-watch": "tsc -p tsconfig.json --watch",
    "coverage": "jest --collect-coverage --passWithNoTests --config=./jest.config.js --runInBand",
    "deploy": "yarn template deploy $@",
    "infra": "yarn template infra",
    "template": "yarn template-ts",
    "template-ts": "ts-node --swc scripts/template.ts",
    "test": "GOLDSTACK_DEPLOYMENT=local jest --passWithNoTests --config=./jest.config.js --runInBand"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "3.620.0",
    "@goldstack/template-s3": "0.5.34",
    "@smithy/smithy-client": "^3.2.0",
    "@smithy/types": "^3.3.0"
  },
  "devDependencies": {
    "@goldstack/template-s3-cli": "0.6.55",
    "@swc/core": "^1.10.1",
    "@swc/jest": "^0.2.37",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.10.2",
    "jest": "^29.7.0",
    "rimraf": "^3.0.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.7.2"
  }
}
