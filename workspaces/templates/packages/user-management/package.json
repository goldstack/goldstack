{
  "name": "@goldstack/user-management",
  "version": "0.1.0",
  "private": true,
  "license": "MIT",
  "author": "Max Rohde",
  "sideEffects": false,
  "main": "src/users.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "yarn clean && yarn compile",
    "clean": "rimraf dist/",
    "compile": "tsc -p tsconfig.json",
    "compile-watch": "tsc -p tsconfig.json --watch",
    "coverage": "jest --collect-coverage --passWithNoTests --config=./jest.config.js --runInBand",
    "deploy": "yarn template-ts deploy $@",
    "infra": "yarn template-ts infra",
    "template": "yarn template-ts",
    "template-ts": "ts-node --project tsconfig.json --swc scripts/template.ts",
    "test": "GOLDSTACK_DEPLOYMENT=local jest --passWithNoTests --config=./jest.config.js --runInBand"
  },
  "dependencies": {
    "@goldstack/template-user-management": "0.1.35"
  },
  "devDependencies": {
    "@goldstack/template-user-management-cli": "0.1.38",
    "@swc/core": "^1.9.3",
    "@swc/jest": "^0.2.37",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.10.0",
    "jest": "^29.3.1",
    "rimraf": "^3.0.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.7.2"
  }
}
