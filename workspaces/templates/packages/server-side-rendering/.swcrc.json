{
  "jsc": {
    "target": "es2020",
    "include": "./../../../../",
    "parser": {
      "syntax": "typescript",
      "tsx": false,
      "decorators": true
    },
    "transform": {
      "decoratorMetadata": true
    },
    "externalHelpers": true
  },
  "module": {
    "type": "commonjs",
    "strict": false,
    "noInterop": false
  },
  "sourceMaps": true,
  "minify": false
}
