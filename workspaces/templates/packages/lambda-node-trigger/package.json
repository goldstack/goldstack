{
  "name": "@goldstack/lambda-node-trigger",
  "version": "0.1.0",
  "private": true,
  "description": "Provides a Node.js based lambda that is triggered by an event or a message",
  "license": "MIT",
  "author": "Max Rohde",
  "sideEffects": false,
  "main": "src/lambda.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "ts-node --swc ./scripts/build.ts",
    "clean": "rimraf 'dist/' && rimraf 'distLambda/'",
    "clean-terraform": "rimraf ./infra/aws/.terraform",
    "compile": "tsc -p tsconfig.json",
    "deploy": "yarn build \"$@\" && yarn template-ts deploy \"$@\"",
    "infra": "yarn template-ts infra",
    "template": "yarn template-ts",
    "template-ts": "ts-node --project tsconfig.json --swc scripts/template.ts",
    "test": "GOLDSTACK_DEPLOYMENT=local jest --passWithNoTests --config=./jest.config.js --runInBand"
  },
  "dependencies": {
    "@aws-sdk/client-sqs": "^3.960.0",
    "@goldstack/template-lambda-trigger-cli": "0.1.59",
    "@goldstack/template-sqs": "0.1.49",
    "@goldstack/utils-log": "0.3.32",
    "@goldstack/utils-sh": "0.5.37",
    "source-map-support": "^0.5.21"
  },
  "devDependencies": {
    "@swc/core": "^1.15.8",
    "@swc/jest": "^0.2.39",
    "@types/aws-lambda": "^8.10.159",
    "@types/jest": "^30.0.0",
    "@types/node": "^25.0.3",
    "@types/source-map-support": "^0.5.10",
    "@yarnpkg/esbuild-plugin-pnp": "^3.0.0-rc.15",
    "esbuild": "^0.25.6",
    "jest": "^30.2.0",
    "rimraf": "^6.1.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.3"
  }
}
