{
  "extends": "../../tsconfig.base.json",
  "include": [
    "./app/**/*",
    "./open-next.config.ts",
    "./app/**/*.json",
    "./schemas/**/*.json",
    "./scripts/**/*",
    "./goldstack.json",
    "./next-env.d.ts",
    ".next/types/**/*.ts",
    "webDist/types/**/*.ts"
  ],
  "compilerOptions": {
    "outDir": "dist",
    "baseUrl": ".",
    "jsx": "preserve",
    "module": "esnext",
    "incremental": true,
    "noEmit": false,
    "lib": ["dom", "dom.iterable", "esnext"],
    "moduleResolution": "node",
    "isolatedModules": true,
    "plugins": [
      {
        "name": "next"
      }
    ]
  },
  "exclude": ["node_modules"],
  "references": [
    {
      "path": "../../../templates-lib/packages/template-nextjs"
    },
    {
      "path": "../../../utils/packages/utils-sh"
    }
  ]
}
