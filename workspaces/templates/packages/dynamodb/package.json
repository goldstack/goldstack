{
  "name": "@goldstack/dynamodb",
  "version": "0.1.0",
  "private": true,
  "license": "MIT",
  "author": "Max Rohde",
  "sideEffects": false,
  "main": "src/table.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "yarn clean && yarn compile",
    "clean": "rimraf dist/",
    "compile": "tsc -p tsconfig.json",
    "compile-watch": "tsc -p tsconfig.json --watch",
    "coverage": "jest --collect-coverage --passWithNoTests --config=./jest.config.js --runInBand",
    "deploy": "yarn node dist/src/template.js deploy $@",
    "infra": "yarn node dist/src/template.js infra",
    "template": "yarn node dist/src/template.js",
    "template-ts": "ts-node src/template.ts",
    "test-ci": "GOLDSTACK_DEPLOYMENT=local jest --passWithNoTests --config=./jest.config.js --runInBand"
  },
  "dependencies": {
    "@aws-sdk/util-dynamodb": "^3.87.0",
    "@goldstack/template-dynamodb": "0.1.13",
    "aws-sdk": "2.1055.0",
    "dynamodb-toolbox": "alpha",
    "umzug": "^3.1.1"
  },
  "devDependencies": {
    "@goldstack/template-dynamodb-cli": "0.4.4",
    "@types/jest": "^27.5.1",
    "@types/node": "^17.0.33",
    "jest": "^28.1.0",
    "rimraf": "^3.0.2",
    "ts-jest": "^28.0.2",
    "ts-node": "^10.8.0",
    "typescript": "^4.6.4"
  }
}
