{
  "name": "@goldstack/dynamodb",
  "version": "0.1.0",
  "private": true,
  "license": "MIT",
  "author": "Max Rohde",
  "sideEffects": false,
  "main": "src/table.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "yarn clean && yarn compile",
    "clean": "rimraf dist/",
    "compile": "tsc -p tsconfig.json",
    "compile-watch": "tsc -p tsconfig.json --watch",
    "coverage": "jest --collect-coverage --passWithNoTests --config=./jest.config.js --runInBand",
    "deploy": "yarn node dist/src/template.js deploy $@",
    "infra": "yarn node dist/src/template.js infra",
    "template": "yarn template-ts",
    "template-ts": "ts-node src/template.ts",
    "test-ci": "GOLDSTACK_DEPLOYMENT=local jest --passWithNoTests --config=./jest.config.js --runInBand",
    "watch": "GOLDSTACK_DEPLOYMENT=local ts-node scripts/watch.ts"
  },
  "dependencies": {
    "@aws-sdk/util-dynamodb": "^3.87.0",
    "@goldstack/template-dynamodb": "0.2.1",
    "aws-sdk": "2.1055.0",
    "dynamodb-toolbox": "^0.4.3",
    "umzug": "^3.1.1"
  },
  "devDependencies": {
    "@goldstack/template-dynamodb-cli": "0.5.1",
    "@types/jest": "^28.1.8",
    "@types/node": "^18.7.13",
    "jest": "^28.1.0",
    "rimraf": "^3.0.2",
    "source-map-support": "^0.5.21",
    "ts-jest": "^28.0.2",
    "ts-node": "^10.9.1",
    "typescript": "^4.7.4"
  }
}
